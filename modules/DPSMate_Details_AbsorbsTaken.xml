	<Button name="DPSMate_Details_AbsorbsTaken_ScrollButton" virtual="true">
		<Size x="220" y="24" />
		<Layers>
			<Layer level="ARTWORT">
				<Texture name="$parent_Icon" file="Interface\AddOns\DPSMate\images\dummy">
					<Size x="24" y="24" />
					<Anchors>
						<Anchor point="TOPLEFT" />
					</Anchors>
				</Texture>
				<FontString name="$parent_Name" inherits="GameFontNormalSmall" text="Sinister Strike Eviscrate" justifyH="LEFT">
					<Size x="110" y="24" />
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parent_Icon" relativePoint="RIGHT">
							<Offset x="5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parent_Value" inherits="GameFontNormalSmall" text="500000 (20.43%)">
					<Size x="70" y="24" />
					<Anchors>
						<Anchor point="RIGHT">
							<Offset x="-5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parent_selected">
					<Anchors>
						<Anchor point="TOPLEFT" />
						<Anchor point="BOTTOMRIGHT" />
					</Anchors>
					<Color r="1" g="1" b="1" a="0.5" />
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>
				local pa, pt = getglobal("DPSMate_Details_AbsorbsTaken_Log_ScrollFrame"), getglobal("DPSMate_Details_AbsorbsTaken_LogTwo_ScrollFrame")
				if this:GetParent():GetName()=="DPSMate_Details_AbsorbsTaken_LogCreature" then
					DPSMate.Modules.DetailsAbsorbsTaken:SelectCreatureButton(pa.index, this:GetID())
				elseif this:GetParent():GetName()=="DPSMate_Details_AbsorbsTaken_Log" then
					DPSMate.Modules.DetailsAbsorbsTaken:SelectCauseButton(pt.index, this:GetID())
				elseif this:GetParent():GetName()=="DPSMate_Details_AbsorbsTaken_LogTwo" then
					DPSMate.Modules.DetailsAbsorbsTaken:SelectCauseABButton(pt.index, pt.indextwo, this:GetID())
				end
			</OnClick>
		</Scripts>
	</Button>
	
